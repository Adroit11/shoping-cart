{
  "name": "yumist",
  "version": "1.0.0",
  "description": "This is a tiny shoping-cart for food delivery project using Express and NodeJS in backend with React & Redux in front-end having porting the data into mysql database.",
  "main": "index.js",
  "scripts": {
    "debug": "node debug app.js",
    "build": "browserify -t [ babelify --presets [  stage-0 react ] ] ./app/yumist.js -o ./public/js/bundle.js",
    "start": "npm run build && node index.js"
  },
  "keywords": [
    "ReactJS",
    "Redux",
    "NodeJs",
    "ExpressJS"
  ],
  "author": "pyadav9678@gmail.com",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.3.15",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "body-parser": "^1.14.1",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "react": "^0.14.3",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.2",
    "redux": "^3.0.4",
    "redux-logger": "^2.2.1",
    "redux-router": "^1.0.0-beta5",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "babelify": "^7.2.0"
  }
